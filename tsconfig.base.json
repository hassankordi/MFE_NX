{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@v-17-2/home": ["libs/home/data-access/src/index.ts"],
      "@v-17-2/home-feature": ["libs/home/feature/src/index.ts"],
      "@v-17-2/home-ui": ["libs/home/ui/src/index.ts"],
      "@v-17-2/login": ["libs/login/feature/src/index.ts"],
      "@v-17-2/login-data-access": ["libs/login/data-access/src/index.ts"],
      "@v-17-2/login-ui": ["libs/login/ui/src/index.ts"],
      "@v-17-2/material-ui-lib": ["libs/shared/src/index.ts"],
      "materialSample/Module": [
        "apps/materialSample/src/app/remote-entry/entry.module.ts"
      ],
      "primengSample/Module": [
        "apps/primengSample/src/app/remote-entry/entry.module.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
